  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "start:ci": "serve --no-clipboard -s build --listen ${PORT:-4000}",
    "start:ci:test": "PORT=4000 react-scripts start",
    "start:ci:chrome": "start-server-and-test start:ci:test http://localhost:4000 cypress:ci:chrome",
    "start:ci:firefox": "start-server-and-test start:ci:test http://localhost:4000 cypress:ci:firefox",
    "build": "react-scripts build",
    "build-ci": "CI=true react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test-ci": "CI=true react-scripts test --watchAll=false --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"./src/**/*.{ts,tsx,json}\" --fix",
    "check-staged": "lint-staged",
    "prettier": "prettier --ignore-path ./.prettierignore --list-different 'src/**/*.{ts,tsx,js,jsx,json}'",
    "fix": "prettier --ignore-path ./.prettierignore --write 'src/**/*.{ts,tsx,js,jsx,json}'",
    "cypress": "TZ=Etc/UTC cypress open",
    "cypress:ci:local": "TZ=Etc/UTC CI=true cypress run --headless",
    "cypress:ci": "CI=true cypress run --config-file cypress.config.ci.ts",
    "cypress:ci:chrome": "CI=true cypress run --config-file cypress.config.ci.ts --browser chrome --headless",
    "cypress:ci:firefox": "CI=true cypress run --config-file cypress.config.ci.ts --browser firefox --headless"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & pretty-quick --staged"
    }
  }
}
